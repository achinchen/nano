{
  "name": "frontend",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "services/frontend",
  "projectType": "application",
  "targets": {
    "build:css": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "unocss 'services/frontend/app/**/*.{ts,tsx}' -o 'services/frontend/app/styles/uno.css' -c 'services/frontend/unocss.config.ts' -m"
        ],
        "forwardAllArgs": true
      }
    },
    "build:next": {
      "executor": "@nx/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "services/frontend/dist"
      },
      "configurations": {
        "development": {
          "outputPath": "services/frontend"
        },
        "production": {}
      }
    },
    "serve:next": {
      "executor": "@nx/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "frontend:build",
        "dev": true
      },
      "configurations": {
        "development": {
          "buildTarget": "frontend:build:next:development",
          "dev": true
        },
        "production": {
          "buildTarget": "frontend:build:next:production",
          "dev": false
        }
      }
    },
    "dev": {
      "executor": "nx:run-commands",
      "outputs": ["{options.outputFile}"],
      "options": {
        "commands": [
          "nx run frontend:build:css --watch",
          "nx run frontend:serve:next:development"
        ],
        "parallel": true
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{options.outputFile}"],
      "options": {
        "commands": [
          "nx run frontend:build:css",
          "nx run frontend:build:next:production"
        ],
        "parallel": false
      }
    },
    "export": {
      "executor": "@nx/next:export",
      "options": {
        "buildTarget": "frontend:build:production"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/{projectRoot}/coverage"],
      "options": {
        "jestConfig": "services/frontend/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["services/frontend/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "build:storybook:css": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "unocss 'services/frontend/app/components/**/*.{ts,tsx,mdx}' -o 'services/frontend/.storybook/styles/uno.css' -c 'services/frontend/unocss.config.ts'"
        ],
        "forwardAllArgs": true
      }
    },
    "dev:storybook": {
      "executor": "@nx/storybook:storybook",
      "options": {
        "port": 4400,
        "configDir": "services/frontend/.storybook"
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "storybook": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["nx build:storybook:css frontend --watch", "nx dev:storybook frontend"],
        "parallel": true
      }
    },
    "build:storybook": {
      "executor": "@nx/storybook:build",
      "outputs": ["{options.outputDir}"],
      "options": {
        "outputDir": "storybook/frontend/dist",
        "configDir": "services/frontend/.storybook"
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      },
      "dependsOn": ["build:storybook:css"]
    },
    "test:storybook": {
      "executor": "nx:run-commands",
      "options": {
        "command": "test-storybook -c services/frontend/.storybook --url=http://localhost:4400"
      }
    },
    "static:storybook": {
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "frontend:build:storybook",
        "staticFilePath": "storybook/frontend/dist"
      },
      "configurations": {
        "ci": {
          "buildTarget": "frontend:build:storybook:ci"
        }
      }
    }
  },
  "tags": []
}
